{
	"name": "sysdiagram",
	"version": "0.1.13",
	"description": "Diagram the system architecture using JavaScript syntax.",
	"main": "dist/sysdiagram.js",
	"unpkg": "dist/sysdiagram.min.js",
	"files": [
		"dist"
	],
	"scripts": {
		"minify": "minify ./lib/sysdiagram.js > ./dist/sysdiagram.min.js",
		"concat": "concat -o ./dist/sysdiagram.js ./lib/sysdiagram.js",
		"docs": "docsify serve ./docs",
		"dev": "http-server -p 4000",
		"build": "npm run concat && npm run minify"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/succeun/sysdiagram"
	},
	"keywords": [
		"d3",
		"graphviz",
		"DOT",
		"graph layout",
		"diagrams",
		"diagram",
		"Viz.js",
		"cloud"
	],
	"author": "Jeong-Ho, Eun",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/succeun/sysdiagram/issues"
	},
	"homepage": "https://succeun.github.io/sysdiagram",
	"dependencies": {
		"docsify": "^4.4.3",
		"minify": "^4.1.1",
		"concat": "^1.0.3",
		"http-server": "^0.12.3"
	}
}
